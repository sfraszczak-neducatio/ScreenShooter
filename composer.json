{
    "name":         "n-educatio/screenshooter",
    "description":  "Module for detecting differences in frontend (based on current and expected screenshot)",
    "keywords":     ["selenium", "webdriver", "screenshot", "testing", "browser"],
    "homepage":     "http://github.com/n-educatio-pl/ScreenShooter",
    "license":      "MIT",

    "authors": [
        {
            "name":      "n-educatio",
            "email":     "biuro@n-educatio.pl",
            "homepage":  "http://n-educatio.pl"
        }
    ],

    "require": {
        "php": ">=5.3.1"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": ">=0.7.2",
        "mikey179/vfsStream": "1.4.*@dev",
        "phpmd/phpmd": "~1.5",
        "zerkalica/phpcs": "dev-master"
    },

    "autoload": {
        "psr-0": {
            "": "src"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "zerkalica/phpcs",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/sfraszczak-neducatio/PHP_CodeSniffer.git",
                    "reference": "master"
                }
            }
        }
    ]
}
